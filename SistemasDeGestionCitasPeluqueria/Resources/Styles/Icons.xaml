<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <!-- Familias de iconos -->
    <OnPlatform x:Key="FluentIconFontFamily" x:TypeArguments="x:String">
        <On Platform="WinUI" Value="Segoe Fluent Icons" />
        <On Platform="Android" Value="FluentSystemIcons" />
        <On Platform="iOS" Value="FluentSystemIcons" />
        <On Platform="MacCatalyst" Value="FluentSystemIcons" />
    </OnPlatform>

    <OnPlatform x:Key="SymbolThemeFontFamily" x:TypeArguments="x:String">
        <On Platform="WinUI" Value="Segoe Fluent Icons" />
        <On Platform="Android" Value="FluentSystemIcons" />
        <On Platform="iOS" Value="FluentSystemIcons" />
        <On Platform="MacCatalyst" Value="FluentSystemIcons" />
    </OnPlatform>

